myList = [12, 45, 3, 22, 67, 34]
def find_maximum(lst):
    if not lst:
        return None
    maximum = lst[0]
    for num in lst:
        if num > maximum:
            maximum = num
    return maximum

maxnum = find_maximum(myList)
print("The maximum number in the list is:", maxnum)
